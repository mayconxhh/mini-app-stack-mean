@font-face
    font-family 'icomoon'
    src url('/fonts/icomoon.eot?ywbqy0')
    src url('/fonts/icomoon.eot?ywbqy0#iefix') format('embedded-opentype'),
    url('/fonts/icomoon.ttf?ywbqy0') format('truetype'),
    url('/fonts/icomoon.woff?ywbqy0') format('woff'),
    url('/fonts/icomoon.svg?ywbqy0#icomoon') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
    font-family 'icomoon' !important
    speak none
    font-style normal
    font-weight normal
    font-variant normal
    text-transform none
    // line-height 1
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

.icon-pencil
    &:before
        content "\e905"

.icon-clock
    &:before
        content "\e94e"

.icon-menu
    &:before
        content "\e9bd"

.icon-cross
    &:before
        content "\ea0f"

.icon-circle-up
    &:before
        content "\ea41"

.icon-circle-down
    &:before
        content "\ea43"